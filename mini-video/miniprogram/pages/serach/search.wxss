.number {
  font-size: 12px;
  display: inline-block;
  width: 16px;
  height: 16px;
  background-color: #e6e6e6;
  color: #878787;
  text-align: center;
  border-radius: 4px;
}
.list_title {
  padding-left: 34rpx;
  font-weight: 500;
  display: inline-block;
}

