/* pages/serach/search.wxss */
.search {
  background-color: #fff;
}

/* 搜索 */
.top {
  height: 102rpx;
  display: flex;
  align-items: center;
  padding: 15rpx 30rpx;
  margin: 10rpx 0;
  border-bottom: 1px solid #cdcdcd;
}

.btn {
  width: 100%;
  height: 80rpx;
  position: relative;
  border-radius: 10rpx;
}

input {
  height: 100%;
  margin-right: 30rpx;
  background-color: #f6f5f7;
  border-radius: 10rpx;
  padding-left: 80rpx;
  color: #b3b3b3;
}

image {
  width: 50rpx;
  height: 50rpx;
  position: absolute;
  left: 2%;
  top: 23%;
}

.back {
  color: #ff6002;
  font-size: 34rpx;
  width: 12%;
  font-size: 500;
  margin-left: 26rpx;
}

/* 导航栏 */
.topnav {
  margin-top: 30rpx ;
  padding: 9rpx 0;
}

swiper {
  height: 80rpx;
  display: flex;
  flex-wrap: nowrap;
}

.keyword {
  font-size: 30rpx;
  width: 125rpx;
  text-align: center;
}

.subnav {
  color: #ff6002;
  position: relative;
  font-size: 30rpx;
  width: 125rpx;
  font-weight: 600;
  text-align: center;
}

.subnav::after {
  content: "";
  position: absolute;
  width: 18px;
  height: 3px;
  background-color: #ff6002;
  top: 25px;
  left: 20px;  
}
/* 关键字 */
.key-words {
  padding: 0 30rpx;
}
.key_list {
  width: 100%;
  border-bottom: 1px solid #ebebeb;
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 15px 0;
  position: relative;
}
.hot_frist {
  display: inline-block;
  width: 19px;
  height: 19px;
  background-color: #e42418;
  text-align: center;
  border-radius: 2px;
  color: #fff;
  font-weight: 500;
font-size: 31rpx;

}
.hot_second {
  display: inline-block;
